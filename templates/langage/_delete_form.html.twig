<form method="post" action="{{ path('app_langage_delete', {'id': langage.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer ce langage ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ langage.id) }}">
    <button class="btnDeleteLangage">Supprimer</button>
</form>
